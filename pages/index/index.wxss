Page{
  height: 100%;
}
.index-page{
  height: 100%;
  background-color: #f8f8f8;
}
.index-page .cates{
  
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  background-color: #fff;
  padding-bottom: 20px;

}

.index-page .cates .list{

  text-align: center;
  width: 20%;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  display: flex;
  margin-top: 20px;
}
.index-page .cates .list .round{
  height: 64px;
  line-height: 64px;
  color: #333;
  width: 64px;
  background-color: #F6CB47;
  border-radius: 50%;
  font-weight: 500;
  font-size: 24px;
  padding: 0;
}
.index-page .cates .list text{
  font-size: 12px;
  color: #666;
  margin-top: 8px;
}

swiper{

  height: 210px;
}
.swiper-img{
  height: 210px;
  width: 100%;
  
}

.header{
  position: relative;
}
.inp-line{
  width: 100%;
  padding: 0 20px;
  top: 15px;
  position: absolute;
  display: flex;
  z-index: 1000;
  box-sizing: border-box;
}
input{
  border-radius: 27px;
  line-height: 40px;
  height: 40px;
  flex: 1;
  background-color: #fff;
  padding: 0 15px;
}